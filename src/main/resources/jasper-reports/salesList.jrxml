    <queryString>
        <![CDATA[SELECT s.sales_id ,s.date,SUM(ds.quantity*p.price) AS total
FROM sales AS s JOIN details_sales AS ds
JOIN products AS p
WHERE s.sales_id=ds.sales_id and ds.product_id=p.id
GROUP BY s.sales_id]]>
    </queryString>
